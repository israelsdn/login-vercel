generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Users{
  id Int @id @default(autoincrement())
  name String @db.VarChar(255)
  email String @db.VarChar(255) @unique
  senha_hash String @db.VarChar(255)

  @@map("user")
}